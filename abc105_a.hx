using Std;
class Main {
	static public function main(){
    	var inputs = Sys.stdin();
        var A = inputs.readLine().split(" ").map(v->v.parseInt());
        var n = A[0];
        var k = A[1];
        if (n%k==0) {
            Sys.print(0);
        } else {
            Sys.print(1);
        }
    }
}
